{
  "name": "v-personal",
  "version": "0.1.0",
  "main": "./dist/vue-personal.common.js",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build-bundle": "vue-cli-service build --target lib --name v-personal ./src/components/index.js",
    "lint": "vue-cli-service lint"
  },
  "description": "A lightweight mobile UI for Vue js",
  "author": "Ronald Aug",
  "license": "MIT",
  "keywords": [
    "vue",
    "components",
    "mobile",
    "UI",
    "lightweight",
    "small"
  ],
  "files": [
    "dist/*",
    "*.json",
    "*.js"
  ],
  "dependencies": {
    "font-awesome": "^4.7.0",
    "node-sass": "^4.11.0",
    "sass-loader": "^7.1.0",
    "vue": "^2.5.17"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.7.0",
    "@vue/cli-plugin-eslint": "^3.7.0",
    "@vue/cli-service": "^3.7.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.2.2",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
